  },
  "license": "MIT",
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "gd-eventlog": "^0.1.15",
    "on-finished": "^2.3.0",
    "prom-client": "^12.0.0",
    "sniffr": "^1.2.0"
  },
  "devDependencies": {
    "snyk": "^1.346.0",
    "@types/express": "^4.17.6",
    "@types/jest": "^25.2.1",
